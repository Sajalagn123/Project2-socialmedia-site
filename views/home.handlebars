{{! example for using useless helper function }}
<h1>Home</h1>

<table id='productsTable'>
  <thead>
    <tr>
      <th>Product Name</th>
      <th>Price</th>
      <th>Description</th>
      <th>Picture</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
      <tr>
        <td>{{product_name}}</td>
        <td>{{price}}</td>
        <td>{{description}}</td>
        <td><img src='{{image}}' alt='{{product_name}}' /></td>
      </tr>
    {{/each}}
  </tbody>
</table>

